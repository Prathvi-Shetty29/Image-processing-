    "import numpy as np\n",
    "wolf=cv2.imread('wolf.jpg')\n",
    "dog=cv2.imread('dog.jpg')\n",
    "wolf_new=wolf.resize(100,100)\n",
    "dog_new=dog.resize(100,100)\n",
    "add=cv2.add(wolf_new,dog_new)\n",
    "cv2.imshow('img',add)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
